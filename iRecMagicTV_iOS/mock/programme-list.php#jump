function search_focus(el)
{
if(el.V) {
if (el.value == el.V) {
el.value = '';
}
} else {
el.V = el.value;
el.value = '';
}
}
function setbg(color)
{
document.bgColor=color;
document.body.style.backgroundColor=color;
}
function jump()
{
window.location='#jump';
}
function send_search(form)
{
var loginpage = "result-programme.php";
var loc = loginpage + "?channelsel=" + 11 + "&keyword="+form.keyword.value;window.location.href = loc;
}
